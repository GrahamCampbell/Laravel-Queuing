{
    "name": "graham-campbell/queuing",
    "description": "Queuing Provides Queuing Performance Increases Laravel 4.2",
    "keywords": ["laravel", "framework", "queuing", "mail", "email", "cron", "queue", "Queuing", "Laravel Queuing", "Laravel-Queuing", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/console": "~4.2",
        "illuminate/queue": "~4.2",
        "illuminate/support": "~4.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "~1.0",
        "pda/pheanstalk": "~2.1",
        "aws/aws-sdk-php": "~2.4",
        "iron-io/iron_mq": "~1.4"
    },
    "suggest": {
        "pda/pheanstalk": "Enables the beanstalk connector.",
        "aws/aws-sdk-php": "Enables the aws connector.",
        "iron-io/iron_mq": "Enables the iron connector."
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Queuing\\": "src/"
        }
