{
    "name": "graham-campbell/queuing",
    "description": "Queuing Is A Cool Way To Queue In Laravel 4.1",
    "keywords": ["laravel", "framework", "queuing", "mail", "email", "cron", "queue", "Queuing", "Laravel Queuing", "Laravel-Queuing", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.1.*",
        "symfony/config": "2.4.*",
        "symfony/filesystem": "2.4.*",
        "graham-campbell/core": "0.2.*",
        "symfony/event-dispatcher": "2.4.*",
        "guzzle/guzzle": "3.8.*",
        "pda/pheanstalk": "2.1.*",
        "aws/aws-sdk-php": "2.4.*",
        "iron-io/iron_mq": "1.4.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "0.2.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "GrahamCampbell\\Queuing": "src/",
            "GrahamCampbell\\Tests\\Queuing": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
